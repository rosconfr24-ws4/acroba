<launch>

  <arg name="controllers" default="finger_state_controller finger1_effort_controller finger2_effort_controller"/>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find acroba_resources)/config/pgc50_controllers.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="pgc50_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="$(arg controllers) --shutdown-timeout 2"/>
 

  <!-- convert joint states to TF transforms for rviz, etc -->
  <!-- Remap to the ones outputted from gazebo-->
  <!--Needed here????-->
  <node name="pgc50_robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
    respawn="false" output="screen">
    <!--<remap from="/joint_states" to="/kr20r1810/joint_states" />-->
  </node>

  


</launch>