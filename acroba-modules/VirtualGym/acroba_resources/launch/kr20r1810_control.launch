<launch>

  <arg name="controllers" default="joint_state_controller PositionJointInterface_trajectory_controller"/>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find acroba_resources)/config/kr20r1810_control_no_namespace.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="$(arg controllers) --shutdown-timeout 2"/>
 

  <!-- convert joint states to TF transforms for rviz, etc -->
  <!-- Remap to the ones outputted from gazebo-->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
    respawn="false" output="screen">
    <!--<remap from="/joint_states" to="/kr20r1810/joint_states" />-->
  </node>

  


</launch>
